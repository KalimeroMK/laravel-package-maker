{
  "name": "abdulwahhabkhan/laravel-package-maker",
  "description": "Get a package skeleton and all other `make` commands from laravel base for package development.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "Abdul Wahhab Khan",
      "email": "abdulwahhabkhan@hotmail.com"
    }
  ],
 
  "autoload": {
    "psr-4": {
      "AWK\\LaravelPackageMaker\\": "./src"
    }
  },
  "minimum-stability": "dev",
  "autoload-dev": {
    "psr-4": {
      "AWK\\LaravelPackageMaker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AWK\\LaravelPackageMaker\\LaravelPackageMakerServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^5.0|^6.0",
    "mockery/mockery": "^1.4"
  }
}
